{
	"name": "vscode-office",
	"displayName": "Office Viewer",
	"description": "View word,excel files in vscode.",
	"version": "1.3.1",
	"publisher": "cweijan",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.40.0"
	},
	"keywords": [
		"office",
		"word",
		"docx",
		"xlsx",
		"excel"
	],
	"categories": [
		"Visualization",
		"Other"
	],
	"bugs": {
		"url": "https://github.com/cweijan/vscode-office/issues",
		"email": "cweijan@163.com"
	},
	"homepage": "https://github.com/cweijan/vscode-office/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/cweijan/vscode-office.git"
	},
	"activationEvents": [
		"onCustomEditor:cweijan.viewOffice"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "reg",
				"aliases": [
					"REG",
					"reg"
				],
				"extensions": [
					".reg",
					".REG"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "reg",
				"scopeName": "source.reg",
				"path": "./syntaxes/reg.tmLanguage"
			}
		],
		"commands": [
			{
				"command": "office.reg.jumpToKey",
				"title": "REG: Jump to registry key"
			}
		],
		"customEditors": [
			{
				"viewType": "cweijan.viewOffice",
				"displayName": "Office Viewer",
				"selector": [
					{
						"filenamePattern": "*.xlsx"
					},
					{
						"filenamePattern": "*.csv"
					},
					{
						"filenamePattern": "*.xls"
					},
					{
						"filenamePattern": "*.docx"
					},
					{
						"filenamePattern": "*.psd"
					},
					{
						"filenamePattern": "*.svg"
					},
					{
						"filenamePattern": "*.epub"
					},
					{
						"filenamePattern": "*.xmind"
					},
					{
						"filenamePattern": "*.pdf"
					},
					{
						"filenamePattern": "*.puml"
					},
					{
						"filenamePattern": "*.plantuml"
					},
					{
						"filenamePattern": "*.pu"
					},
					{
						"filenamePattern": "*.md"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"postinstall": "",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "^13.11.0",
		"@types/node-fetch": "^2.5.7",
		"@types/vscode": "^1.40.0",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"date-format": "^3.0.0",
		"mammoth": "^1.4.13",
		"node-fetch": "^2.6.1",
		"pretty-markdown-pdf": "1.4.0"
	}
}
