{
	"name": "vscode-office",
	"displayName": "Office Viewer",
	"description": "View word,excel files in vscode.",
	"version": "1.4.3",
	"publisher": "cweijan",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.40.0"
	},
	"keywords": [
		"office",
		"word",
		"docx",
		"xlsx",
		"excel"
	],
	"categories": [
		"Visualization",
		"Other"
	],
	"bugs": {
		"url": "https://github.com/cweijan/vscode-office/issues",
		"email": "cweijan@163.com"
	},
	"homepage": "https://github.com/cweijan/vscode-office/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/cweijan/vscode-office.git"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "reg",
				"aliases": [
					"REG",
					"reg"
				],
				"extensions": [
					".reg",
					".REG"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "http",
				"aliases": [
				  "HTTP",
				  "Http",
				  "http"
				],
				"extensions": [
				  ".http",
				  ".rest"
				],
				"firstLine": "^(GET|POST|PUT|DELETE|PATCH|HEAD|OPTIONS|CONNECT|TRACE)\\s+(.*?)HTTP/[0-9\\.]+$",
				"configuration": "./http-language-configuration.json"
			  }
		],
		"grammars": [
			{
				"language": "reg",
				"scopeName": "source.reg",
				"path": "./syntaxes/reg.tmLanguage"
			},
			{
				"language": "http",
				"scopeName": "source.http",
				"path": "./syntaxes/http.tmLanguage.json"
			  }
		],
		"snippets": [
			{
			  "language": "http",
			  "path": "./snippets/http.json"
			}
		  ],
		"commands": [
			{
				"command": "office.reg.jumpToKey",
				"title": "REG: Jump to registry key"
			},
			{
				"command": "office.markdown.paste",
				"title": "Enhance pater in markdown"
			}
		],
		"keybindings": [
			{
				"command": "office.markdown.paste",
				"key": "ctrl+v",
				"when": "!terminalFocus && !filesExplorerFocus"
			}
		],
		"customEditors": [
			{
				"viewType": "cweijan.viewOffice",
				"displayName": "Office Viewer",
				"selector": [
					{
						"filenamePattern": "*.xlsx"
					},
					{
						"filenamePattern": "*.csv"
					},
					{
						"filenamePattern": "*.xls"
					},
					{
						"filenamePattern": "*.docx"
					},
					{
						"filenamePattern": "*.psd"
					},
					{
						"filenamePattern": "*.svg"
					},
					{
						"filenamePattern": "*.epub"
					},
					{
						"filenamePattern": "*.xmind"
					},
					{
						"filenamePattern": "*.pdf"
					},
					{
						"filenamePattern": "*.puml"
					},
					{
						"filenamePattern": "*.plantuml"
					},
					{
						"filenamePattern": "*.pu"
					}
				]
			},
			{
				"viewType": "cweijan.viewMarkdown",
				"displayName": "Markdown Viewer",
				"selector": [
					{
						"filenamePattern": "*.md"
					}
				]
			}
		],
		"configuration": {
			"type": "object",
			"title": "Office",
			"properties": {
				"vscode-office.markdownType": {
					"type": "string",
					"enum": [
						"hyperMD",
						"stackedit"
					],
					"default": "hyperMD",
					"description": "Set markdown viewer type."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"install": "npm i --ignore-scripts",
		"publish": "vsce package --yarn"
	},
	"devDependencies": {
		"@types/node": "^13.11.0",
		"@types/node-fetch": "^2.5.7",
		"@types/vscode": "^1.40.0",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"chrome-finder": "^1.0.7",
		"mammoth": "^1.4.13",
		"node-fetch": "^2.6.1"
	}
}
