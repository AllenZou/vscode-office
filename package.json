{
	"name": "vscode-office",
	"displayName": "Office Viewer",
	"description": "View word,excel files in vscode.",
	"version": "1.0.0",
	"publisher": "cweijan",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.40.0"
	},
	"keywords": [
		"office",
		"word",
		"docx",
		"xlsx",
		"excel"
	],
	"categories": [
		"Visualization",
		"Other"
	],
	"bugs": {
		"url": "https://github.com/cweijan/vscode-office/issues",
		"email": "cweijan@163.com"
	},
	"homepage": "https://github.com/cweijan/vscode-office/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/cweijan/vscode-office.git"
	},
	"activationEvents": [
		"onCustomEditor:cweijan.viewOffice"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "cweijan.viewOffice",
				"displayName": "Office Viewer",
				"selector": [
					{
						"filenamePattern": "*.xlsx"
					},
					{
						"filenamePattern": "*.xls"
					},
					{
						"filenamePattern": "*.docx"
					},
					{
						"filenamePattern": "*.doc"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.40.0",
		"@types/node": "^13.11.0",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"date-format": "^3.0.0",
		"mammoth": "^1.4.13"
	}
}
