{
	"name": "vscode-office",
	"displayName": "Office Viewer",
	"description": "View word,excel files in vscode.",
	"version": "1.4.0",
	"publisher": "cweijan",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.40.0"
	},
	"keywords": [
		"office",
		"word",
		"docx",
		"xlsx",
		"excel"
	],
	"categories": [
		"Visualization",
		"Other"
	],
	"bugs": {
		"url": "https://github.com/cweijan/vscode-office/issues",
		"email": "cweijan@163.com"
	},
	"homepage": "https://github.com/cweijan/vscode-office/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/cweijan/vscode-office.git"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "reg",
				"aliases": [
					"REG",
					"reg"
				],
				"extensions": [
					".reg",
					".REG"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "reg",
				"scopeName": "source.reg",
				"path": "./syntaxes/reg.tmLanguage"
			}
		],
		"commands": [
			{
				"command": "office.reg.jumpToKey",
				"title": "REG: Jump to registry key"
			},
			{
				"command": "office.markdown.paste",
				"title": "Enhance pater in markdown"
			}
		],
		"keybindings": [
			{
				"command": "office.markdown.paste",
				"key": "ctrl+v"
			}
		],
		"customEditors": [
			{
				"viewType": "cweijan.viewOffice",
				"displayName": "Office Viewer",
				"selector": [
					{
						"filenamePattern": "*.xlsx"
					},
					{
						"filenamePattern": "*.csv"
					},
					{
						"filenamePattern": "*.xls"
					},
					{
						"filenamePattern": "*.docx"
					},
					{
						"filenamePattern": "*.psd"
					},
					{
						"filenamePattern": "*.svg"
					},
					{
						"filenamePattern": "*.epub"
					},
					{
						"filenamePattern": "*.xmind"
					},
					{
						"filenamePattern": "*.pdf"
					},
					{
						"filenamePattern": "*.puml"
					},
					{
						"filenamePattern": "*.plantuml"
					},
					{
						"filenamePattern": "*.pu"
					},
					{
						"filenamePattern": "*.md"
					}
				]
			}
		]
	},
	"configuration": {
		"type": "object",
		"title": "OfficeViewer",
		"properties": {
			"vscode-office.markdownType": {
				"type": [
					"default",
					"stackedit",
					"marktext(UnSupport)"
				],
				"default": "default",
				"description": "Set markdown viewer type."
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"postinstall": " echo 1  ",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "^13.11.0",
		"@types/node-fetch": "^2.5.7",
		"@types/vscode": "^1.40.0",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"mammoth": "^1.4.13",
		"node-fetch": "^2.6.1"
	}
}
